<?xml version="1.0" encoding="UTF-8"?>
<project name="jarvis-march" default="DLLibs" basedir="."> 
  <target name="DLLibs">
  	<!-- Create the /lib folder if it doesn't exist. -->
  	<mkdir dir="lib"/>
  	<!-- Downloads external JAR dependencies. -->
  	<get src="http://www.migcalendar.com/miglayout/versions/4.0/miglayout-4.0-swing.jar"
  	     dest="lib"
  	     verbose="true"
  	     skipexisting="true"/>
  	<get src="http://www.migcalendar.com/miglayout/versions/4.0/miglayout-4.0-sources.jar"
         dest="lib"
         verbose="true"
         skipexisting="true"/>
	<get src="http://www.migcalendar.com/miglayout/versions/4.0/miglayout-4.0-javadoc.jar"
         dest="lib"
         verbose="true"
         skipexisting="true"/>
  </target>
</project>